#!/usr/bin/env node
var debug = require('debug')('KFSJ');
var app = require('../app');

var server = app.listen(3000, function() {
    debug('Express server listening on port ' + server.address().port);
}).on('error', function (e) {
    if (e.code == 'EADDRINUSE') {
        debug('Address in use. Is the server already running?');
    }
});
